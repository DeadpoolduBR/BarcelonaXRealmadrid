import time
import os
import random

def warning_message():
    messages = [
        "âš ï¸ BLOCK: Your system is under verification...",
        "ğŸ”’ ATENCION: Security breach detected!",
        "ğŸš¨ CERTIFICAD ERROR: Your access is restricted.",
        "ğŸ’€ WARNING: Unauthorized operation detected!",
        "ğŸ“› SYSTEM BLOCKED: Awaiting security clearance...",
        "ğŸ”¥ ALERTA: Possible malware infection detected!",
    ]
    for _ in range(10):  # Loop para exibir mensagens falsas
        print(random.choice(messages))
        time.sleep(1)

def fake_scan():
    print("\nğŸ” Scanning system files...\n")
    for _ in range(5):
        print(f"Checking file: C:/system32/{random.randint(10000,99999)}.dll")
        time.sleep(0.5)
    print("\nâŒ ERROR: Critical files compromised!")
    time.sleep(2)
    print("\nğŸ”„ Attempting automatic repair...\n")
    time.sleep(3)
    print("âœ… System restored. Please restart your device.")

def main():
    os.system("cls" if os.name == "nt" else "clear")  # Limpa a tela
    warning_message()
    fake_scan()

if __name__ == "__main__":
    main()
